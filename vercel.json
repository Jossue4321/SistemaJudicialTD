{
  "version": 2,
  "builds": [
    {
      "src": "scripts/ml_question_recommender.py",
      "use": "@vercel/python",
      "config": {
        "maxDuration": 10
      }
    }
  ],
  "env": {
    "PYTHON_PATH": "python3",
    "PYTHON_VERSION": "3.9"
  },
  "routes": [
    {
      "src": "/api/chatbot",
      "dest": "scripts/ml_question_recommender.py"
    }
  ]
}